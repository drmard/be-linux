				     <GIC_SPI 322 IRQ_TYPE_LEVEL_HIGH>, /* ecc_corrected_err_fault */
				     <GIC_SPI 321 IRQ_TYPE_LEVEL_HIGH>; /* ecc_uncorrected_err_fault */
			// clocks = <&cmu_ddr_0 0>;
