			compatible = "baikal,bs1000-edac-mc";
			reg = <0x0 0x53000000 0x0 0x10000>;
			interrupts = <GIC_SPI 326 IRQ_TYPE_LEVEL_HIGH>, /* dfi_alert_err */
				     <GIC_SPI 324 IRQ_TYPE_LEVEL_HIGH>, /* ecc_corrected_err */
